{
  "manifest_version": 2,
  "name": "Comic Sans Everything",
  "description": "Extention that turns all website font to Comic Sans MS.",
  "version": "2.0",
  "homepage_url": "https://www.facebook.com/ComicSansEverything",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Change Comic Sans settings",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["background.js"]
  }]
}
